{
  "name": "serverless-typescript-template",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "lodash": "^4.17.21",
    "mysql2": "^3.6.1",
    "p-map": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.2.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "scripts": {
    "api-dev": "sls offline start --stage dev",
    "api-prod": "sls offline start --stage prod",
    "deploy-dev": "chmod +x ./deploy.sh && ./deploy.sh dev",
    "deploy-prod": "chmod +x ./deploy.sh && ./deploy.sh prod",
    "fix": "npm run fix-formatting && npm run fix-lint && npx tsc",
    "fix-lint": "eslint './**/*.ts' --quiet --fix",
    "fix-formatting": "npx prettier --write './**/*.{md,ts}'",
    "typeorm-dev": "npx dotenv -c dev typeorm-ts-node-commonjs -- -d src/data-source.ts",
    "typeorm-prod": "npx dotenv -c prod typeorm-ts-node-commonjs -- -d src/data-source.ts",
    "script-dev": "npx dotenv -c dev ts-node",
    "script-prod": "npx dotenv -c prod ts-node"
  },
  "devDependencies": {
    "@types/airtable": "^0.10.1",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "esbuild": "^0.17.18",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.8",
    "serverless": "^3.30.1",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
